#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/lk/svelte5-commerce/node_modules/.pnpm/@sveltejs+kit@2.5.8_@sveltejs+vite-plugin-svelte@3.0.2_svelte@5.0.0-next.133_vite@5.2.7/node_modules/@sveltejs/kit/node_modules:/mnt/d/lk/svelte5-commerce/node_modules/.pnpm/@sveltejs+kit@2.5.8_@sveltejs+vite-plugin-svelte@3.0.2_svelte@5.0.0-next.133_vite@5.2.7/node_modules/@sveltejs/node_modules:/mnt/d/lk/svelte5-commerce/node_modules/.pnpm/@sveltejs+kit@2.5.8_@sveltejs+vite-plugin-svelte@3.0.2_svelte@5.0.0-next.133_vite@5.2.7/node_modules:/mnt/d/lk/svelte5-commerce/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/lk/svelte5-commerce/node_modules/.pnpm/@sveltejs+kit@2.5.8_@sveltejs+vite-plugin-svelte@3.0.2_svelte@5.0.0-next.133_vite@5.2.7/node_modules/@sveltejs/kit/node_modules:/mnt/d/lk/svelte5-commerce/node_modules/.pnpm/@sveltejs+kit@2.5.8_@sveltejs+vite-plugin-svelte@3.0.2_svelte@5.0.0-next.133_vite@5.2.7/node_modules/@sveltejs/node_modules:/mnt/d/lk/svelte5-commerce/node_modules/.pnpm/@sveltejs+kit@2.5.8_@sveltejs+vite-plugin-svelte@3.0.2_svelte@5.0.0-next.133_vite@5.2.7/node_modules:/mnt/d/lk/svelte5-commerce/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
fi
