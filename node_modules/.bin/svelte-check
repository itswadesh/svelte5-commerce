#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/lk/svelte5-commerce/node_modules/.pnpm/svelte-check@3.6.8_@babel+core@7.24.5_postcss-load-config@5.0.3_postcss@8.4.38_svelte@5.0.0-next.133/node_modules/svelte-check/bin/node_modules:/mnt/d/lk/svelte5-commerce/node_modules/.pnpm/svelte-check@3.6.8_@babel+core@7.24.5_postcss-load-config@5.0.3_postcss@8.4.38_svelte@5.0.0-next.133/node_modules/svelte-check/node_modules:/mnt/d/lk/svelte5-commerce/node_modules/.pnpm/svelte-check@3.6.8_@babel+core@7.24.5_postcss-load-config@5.0.3_postcss@8.4.38_svelte@5.0.0-next.133/node_modules:/mnt/d/lk/svelte5-commerce/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/lk/svelte5-commerce/node_modules/.pnpm/svelte-check@3.6.8_@babel+core@7.24.5_postcss-load-config@5.0.3_postcss@8.4.38_svelte@5.0.0-next.133/node_modules/svelte-check/bin/node_modules:/mnt/d/lk/svelte5-commerce/node_modules/.pnpm/svelte-check@3.6.8_@babel+core@7.24.5_postcss-load-config@5.0.3_postcss@8.4.38_svelte@5.0.0-next.133/node_modules/svelte-check/node_modules:/mnt/d/lk/svelte5-commerce/node_modules/.pnpm/svelte-check@3.6.8_@babel+core@7.24.5_postcss-load-config@5.0.3_postcss@8.4.38_svelte@5.0.0-next.133/node_modules:/mnt/d/lk/svelte5-commerce/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
